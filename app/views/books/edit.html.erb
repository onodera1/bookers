

<h1>Editing Book</h1>
<!-- form_for部分 -->
<%= form_for(@book) do |f| %>
    <h4>Title</h4>
    <%= f.text_field :title %>
    <h4>Opinion</h4>
    <%= f.text_area :body %>
    <%= f.submit 'Update Book' %>
<% end %>

<tr>
<td><%= link_to "show", book_path(@book) %></td>
<td><%= link_to "Back", book_path %></td>
</tr>